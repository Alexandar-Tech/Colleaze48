{
  "expo": {
    "name": "Colleaze",
    "slug": "Colleaze",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nizcare.colleaze",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alex_mamce.Colleaze"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "83930076-4712-4843-b6e0-da7fc1e93f27"
      }
    }
  }
}
